int mul(int a,int b,char c)
{
	switch(c)
	{
		case '*':{
			return a*b;
			break;
		}
		case '/' :{
		return a/b;
		}
		case '+':{
			return a+b;
			break;
		}
		case '-':{
			return a-b;
			break;
		} 
}
}
